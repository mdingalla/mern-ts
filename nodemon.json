{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "watch": [
    "client",
    "server",
    "views"
  ],
  "verbose": true,
  "legacyWatch": true,
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts js json",
  "exec": "npm run build-server && node dist/server/server.js"
}
